<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ting.mometest.Activity">
    //授予APP拨号权限
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">

        </activity>
        <activity android:name=".WelcomeActivity"
            android:theme="@style/FullScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ContentActivity" />
        <activity android:name=".BaseActivity" />
        <activity android:name=".CreateActivity" />
        <activity android:name=".VoiceCreateActivity" />
        <activity android:name=".SearchActivity" />
        <activity android:name=".CalendarActivity" />
        <activity android:name=".CallActivity" />
        <activity android:name=".AllNotesActivity" />
        <activity android:name=".RecordPlayerActivity" />
        <activity android:name=".RecycleActivity" />
        <activity android:name=".SettingActivity" />
        <activity android:name=".SecurityActivity" />
        <activity android:name=".AlertActivity" />

        <receiver
            android:name="com.example.ting.mometest.Manager.AlarmReceiver"
            android:process=":remote" />

        <service
            android:name="com.example.ting.mometest.Manager.AlertService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".GuideActivity"
            android:theme="@style/FullScreen"></activity>
    </application>

</manifest>